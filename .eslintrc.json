{
    "env": {
	"commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-undef": "error",
	"no-unused-vars": "warn"
    }
}
